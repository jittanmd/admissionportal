
<%- include('../header.ejs') %>
<br><br><br><br><br>
<div class="container mt-5">
    <div class="row">
        <div class="col-md-12">
           <table class="table table-bordered">
            <tr>
                <th>S.No</th>
                <th>Name</th>
                <th>Email</th>
                <th>status</th>
                <th>action</th>
            </tr>
            <% i=1 %>
            <% p.forEach((c)=>{ %>
              <tr>
                <td> <%=c._id %> </td>
                <td><%= c.name %></td>
                <td><%= c.email %></td>
                <td><%= c.status %></td>
                <td>
                    <a href="/courseview/<%= c._id %>" class="btn btn-info">view</a>
                    <a href="/coursedelete/<%= c._id %>" class="btn btn-danger">delete</a>
                </td>
              </tr>
            <% }) %>
           </table>
        </div>
    </div>
</div>
<%- include('../footer.ejs') %>